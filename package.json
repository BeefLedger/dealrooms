{
  "name": "investor-engine",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "ethers": "^5.0.7",
    "global": "^4.4.0",
    "truffle": "^5.1.35",
    "web3": "^1.2.11",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "latest",
    "@types/jest": "^26.0.7",
    "@types/yargs": "^15.0.5",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.4",
    "typechain": "^2.0.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "test": "jest",
    "contracts:type": "yarn typechain --target ./node_modules/@typechain/ethers-v5 --outDir ./src/types './build/contracts/*.json'",
    "contracts:clear": "rm -rf ./abis && rm -rf ./types",
    "contracts:build": "yarn contracts:clear && yarn contracts:compile && yarn contracts:type",
    "contracts:compile": "yarn truffle compile",
    "build:src": "tsc",
    "test:contracts": "jest --detectOpenHandles --forceExit --runInBand --testPathPattern=./test",
    "cli:contract": "ts-node ./cli/contract.ts",
    "cli": "yarn cli:contract deploy all"
  },
