{
  "name": "investor-engine",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@typechain/ethers-v4": "^1.0.1",
    "ethers": "5",
    "ganache-cli": "^6.10.1",
    "global": "^4.4.0",
    "truffle": "^5.1.35",
    "web3": "^1.2.11",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^1.0.0",
    "@types/jest": "^26.0.7",
    "@types/yargs": "^15.0.5",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.4",
    "typechain": "^2.0.0",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "test": "jest --detectOpenHandles --forceExit --runInBand",
    "contracts:type": "yarn typechain --target ./node_modules/@typechain/ethers-v5 --outDir ./src/types './src/abi/**/*.json'",
    "contracts:clear": "rm -rf ./src/abis && rm -rf ./src/types",
    "contracts:build": "yarn contracts:clear && yarn contracts:compile && yarn contracts:type",
    "contracts:compile": "yarn truffle compile",
    "src:build": "rm -rf ./dist && tsc",
    "build": "yarn contracts:build && yarn src:build",
    "remix": "remixd -s ./contracts --remix-ide https://remix.ethereum.org"
  },
